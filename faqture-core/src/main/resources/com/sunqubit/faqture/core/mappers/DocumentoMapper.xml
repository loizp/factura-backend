<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD//Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'>
<mapper namespace="com.sunqubit.faqture.core.mappers.DocumentoMapper">
    <resultMap id="DocumentoMap" type="documento">
        <id column="clie_id" property="clieId" />
        <result column="clie_numero" property="clieNumero" />   
        <result column="clie_nombres" property="clieNombres"/>   
        
    </resultMap>

    
    <select id="getListE" resultMap="DocumentoMap">
        select * from sunqubit.documentos
    </select>

</mapper>
